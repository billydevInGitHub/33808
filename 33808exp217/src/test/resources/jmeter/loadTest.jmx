            <stringProp name="BeanShellSampler.query">import java.lang.StringBuilder;
import java.util.Random;

public static String DATA = &quot;abcdefghijklmnopqrstuvwxzy&quot;;
public static Random RANDOM = new Random();

public String randomString(int len) {
	StringBuilder result = new StringBuilder(len);
	for (int i = 0; i &lt; len; i++) {
		result.append(DATA.charAt(RANDOM.nextInt(DATA.length())));
	}
	return result.toString(); 
}

public String randomCurrency(int bound, int min) {
	int value = RANDOM.nextInt(bound);
	double result = RANDOM.nextInt(bound) / 10.0 + RANDOM.nextInt(bound) + min;
	return String.format(&quot;%.2f&quot;, new Object[] { result });
}

public String[] randomCategories(int maxCategories) {
	int categoriesNo = RANDOM.nextInt(maxCategories);
	if (categoriesNo == 0) {
		categoriesNo++;
	}

	String[] result = new String[categoriesNo];
	for (int i = 0; i &lt; categoriesNo; i++) {
		result[i] = randomString(6) + i;
	}

	return result;
}

public String categoriesRequestParameter() {
	String result = &quot;&quot;;
	String[] categories = randomCategories(20);
	for (int i = 0; i &lt; categories.length; i++) {
		if (i &gt; 0) {
			result += &quot;&amp;&quot;;
		}
		result += &quot;category=&quot; + categories[i];
	}
	return result;
}

vars.put(&quot;MIN_RENTAL_RATE&quot;, randomCurrency(3, 0));
vars.put(&quot;MAX_RENTAL_RATE&quot;, randomCurrency(10, 3));
vars.put(&quot;CATEGORIES&quot;, categoriesRequestParameter());
</stringProp>
